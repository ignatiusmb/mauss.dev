{
  "name": "mauss.dev",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "lint": "svelte-kit sync && npm run lint:es && npm run lint:sv && npm run lint:ts && npm run lint:md",
    "lint:es": "eslint src --ext js,ts,svelte",
    "lint:sv": "svelte-check src --tsconfig tsconfig.json",
    "lint:ts": "tsc --strict --noEmit --skipLibCheck",
    "lint:md": "markdownlint content/**/*.md",
    "check": "prettier -c src",
    "format": "prettier -w src"
  },
  "prettier": "mauss/prettier.json",
  "devDependencies": {
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "katex": "^0.16.2",
    "markdown-it-texmath": "^1.0.0",
    "markdownlint-cli": "^0.32.2",
    "marqua": "^0.3.1",
    "mauss": "^0.3.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.51.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "syv": "^0.0.15",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
}
