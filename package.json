{
  "name": "mauss.dev",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "lint": "svelte-kit sync && npm run lint:es && npm run lint:sv && npm run lint:ts && npm run lint:md",
    "lint:es": "eslint src --ext js,ts,svelte",
    "lint:sv": "svelte-check src --tsconfig tsconfig.json",
    "lint:ts": "tsc --strict --noEmit --skipLibCheck",
    "lint:md": "markdownlint content/**/*.md",
    "check": "prettier -c src",
    "format": "prettier -w src"
  },
  "prettier": "mauss/prettier.json",
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.2.4",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "katex": "^0.16.0",
    "markdown-it-texmath": "^1.0.0",
    "markdownlint-cli": "^0.32.1",
    "marqua": "^0.3.0",
    "mauss": "^0.3.0",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.1",
    "svelte": "^3.32.1",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.6.5",
    "syv": "^0.0.14",
    "typescript": "^4.1.3",
    "vite": "^3.0.3"
  }
}
