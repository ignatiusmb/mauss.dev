{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "format": "prettier -w src",
    "check": "svelte-kit sync && pnpm check:style && pnpm check:lint && pnpm check:svelte && pnpm check:types && pnpm check:content",
    "check:style": "prettier -c src",
    "check:lint": "eslint src --ext js,ts,svelte",
    "check:svelte": "svelte-check src --tsconfig tsconfig.json",
    "check:types": "tsc --strict --noEmit --skipLibCheck",
    "check:content": "markdownlint content/**/*.md"
  },
  "packageManager": "pnpm@7.13.4",
  "prettier": "mauss/prettier.json",
  "devDependencies": {
    "@sveltejs/adapter-vercel": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "katex": "^0.16.2",
    "markdown-it-texmath": "^1.0.0",
    "markdownlint-cli": "^0.32.2",
    "marqua": "^0.3.1",
    "mauss": "^0.3.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.51.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "syv": "^0.0.15",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
}
