{
  "name": "mauss.dev",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "prepare": "svelte-kit sync",
    "lint": "npm run lint:es && npm run lint:sv && npm run lint:ts && npm run lint:md",
    "lint:es": "eslint src --ext js,ts,svelte",
    "lint:sv": "svelte-check src --tsconfig tsconfig.json",
    "lint:ts": "tsc --strict --noEmit --skipLibCheck",
    "lint:md": "markdownlint content/**/*.md",
    "check": "prettier -c src",
    "format": "prettier -w src"
  },
  "prettier": "mauss/prettier.json",
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "autoprefixer": "^10.2.4",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-svelte3": "^3.0.0",
    "katex": "^0.15.2",
    "markdown-it-texmath": "^0.9.0",
    "markdownlint-cli": "^0.31.1",
    "marqua": "^0.3.0",
    "mauss": "^0.2.3",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.1",
    "svelte": "^3.32.1",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.6.5",
    "syv": "latest",
    "typescript": "^4.1.3"
  }
}
