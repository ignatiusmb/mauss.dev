{
	"extends": "mauss/tsconfig.json",
	"compilerOptions": {
		"lib": ["DOM", "ES2020", "WebWorker"],
		"types": ["svelte"],
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"$app/*": [".svelte/dev/runtime/app/*", ".svelte/build/runtime/app/*"],
			"$lib/*": ["src/lib/*"]
		}
	},
	"include": ["src/**/*.js", "src/**/*.ts", "src/**/*.svelte"]
}
